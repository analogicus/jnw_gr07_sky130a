* Measure temp_to_current
.control

*load {cicname}.raw

echo "MEAS_START"

*let iout = -i(vmeasure)
*meas dc iout_meas find iout at=5n

set fend = .raw
foreach vtemp -40 -20 0 20 40 60 80 100 120
    option temp=$vtemp
    load {cicname}_$vtemp$fend
    meas tran iout_meas_$vtemp find i(vmeasure) at=5u
end

echo "MEAS_END"
.endc
